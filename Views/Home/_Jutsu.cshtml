@model Jutsu

<h3>Add a Jutsu</h3>

<form action="/jutsu" method="post">
    <div class="form-group">
        <label>Technique Name</label>
        <input type="text" name="TechniqueName" class="form-control">
        <span asp-validation-for="TechniqueName"></span>
    </div>
    <input type="submit" class="btn btn-danger">
</form>